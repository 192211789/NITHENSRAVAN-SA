#include<stdio.h>
int main()
{
	int a,b,x,n;
	printf("choose anumber between 1 to 4");
	scanf("%d,&n");
	printf("entera,b values");
	scanf("%d%d",&a,&b);
	switch(n)
	{
		case 1:x=a+b;
		break;
		case 2:x=a-b;
		break;
		case 3:x=a*b;
		break;
		case 4:x=a/b;
		break;
		case 5:x=a%b;
		break;
		default:printf("enter the correct symbol");
		break;
	}
}
